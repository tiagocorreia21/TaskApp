{
  "name": "bulletproof-nodejs",
  "version": "1.0.0",
  "description": "Bulletproof node.js",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "heroku-postbuild": "npm run build",
    "prestart": "npm run kill-port",
    "start": "nodemon",
    "inspect": "nodemon --inspect src/app.ts",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "test:unit": "mocha -r ts-node/register tests/unit/**/*.test.ts",
    "test:integration": "mocha -r ts-node/register tests/integration/**/*.test.ts --timeout 10000",
    "test:system": "mocha -r ts-node/register tests/integration/*.system.test.ts --timeout 15000",
    "test:incidentType": "mocha -r ts-node/register tests/**/*incidentType*.test.ts --timeout 15000",
    "test:incidentType:unit": "mocha -r ts-node/register tests/unit/incidentType.domain.test.ts",
    "test:incidentType:aggregate": "mocha -r ts-node/register tests/integration/incidentType.aggregate.test.ts",
    "test:incidentType:application": "mocha -r ts-node/register tests/integration/incidentType.application.test.ts --timeout 30000",
    "test:incidentType:system": "mocha -r ts-node/register tests/integration/incidentType.system.test.ts --timeout 15000",
    "test:incident": "mocha -r ts-node/register tests/**/*incident.*.test.ts --timeout 15000",
    "test:incident:unit": "mocha -r ts-node/register tests/unit/incident.domain.test.ts",
    "test:incident:aggregate": "mocha -r ts-node/register tests/integration/incident.aggregate.test.ts",
    "test:incident:application": "mocha -r ts-node/register tests/integration/incident.application.test.ts --timeout 30000",
    "test:incident:system": "mocha -r ts-node/register tests/integration/incident.system.test.ts --timeout 15000",
    "test:incidents:all": "mocha -r ts-node/register tests/**/*incident*.test.ts --timeout 15000",
    "test:incidents:domain": "mocha -r ts-node/register tests/unit/*incident*.test.ts",
    "test:incidents:aggregate": "mocha -r ts-node/register tests/integration/*incident*.aggregate.test.ts",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "kill-port": "npx kill-port 4000 || echo \"Port 4000 is free\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/santiq/bulletproof-nodejs.git"
  },
  "keywords": [
    "boilerplay",
    "cron",
    "jobs",
    "js",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "author": "Santiago Quinteros",
  "license": "ISC",
  "dependencies": {
    "acorn": "^7.1.0",
    "acorn-jsx": "^5.1.0",
    "agenda": "^5.0.0",
    "agendash": "^4.0.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "celebrate": "^15.0.3",
    "chai": "^4.2.0",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^3.1.0",
    "express": "^4.22.1",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^8.4.1",
    "express-oauth2-jwt-bearer": "^1.7.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.13",
    "method-override": "^3.0.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "mongodb": "^4.17.2",
    "mongoose": "^6.1.0",
    "mongoose-sequence": "^6.0.1",
    "morgan": "^1.9.1",
    "nodemon": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "shallow-equal-object": "^1.1.1",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typedi": "^0.10.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.118",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.11.12",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.0.0",
    "kill-port": "^2.0.1",
    "mocha": "^10.1.0",
    "mongodb-memory-server": "^10.3.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.4.0",
    "typescript": "^5.7.2"
  },
  "bugs": {
    "url": "https://github.com/santiq/bulletproof-nodejs/issues"
  },
  "homepage": "https://github.com/santiq/bulletproof-nodejs#readme"
}
